{
  "name": "map2online",
  "version": "1.0.0",
  "description": "Map2Online is online service intended to be used for planning trips and travels",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "webpack-dev-server --color --config webpack.config-dev.js",
    "build": "webpack --config webpack.config-prod.js",
    "stats": "webpack --config webpack.config-stats.js --profile --json >stats.json",
    "tests": "npm npm run test:components",
    "unit-tests:types": "npm run ts-mocha unit-tests/types/**/*.test.ts",
    "unit-tests:components": "jest --runInBand src/react/components",
    "unit-tests:imports": "npm run mocha --require ./mocha/imports/setup.js \"mocha/imports/**/*.test.js\"",
    "unit-tests:reducers": "npm run mocha \"mocha/reducers/**/*.test.js\"",
    "unit-tests:contexts": "npm run ts-mocha \"unit-tests/contexts/**/*.test.ts\"",
    "unite-tests:acceptance": "npm run testcafe",
    "test:mocha": "mocha -r ts-node/register tests/mocha/**/*.spec.ts",
    "testcafe": "testcafe chrome testcafe/** --app \"http-server ./dist -s\"",
    "jest": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.s4y:s4ysolutions/map2online.git"
  },
  "author": "S4Y",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-react-perf": "^3.2.3",
    "file-loader": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.0",
    "node-sass": "^4.13.1",
    "npm-watch": "^0.6.0",
    "react-hot-loader": "^4.12.19",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "file-saver": "^2.0.2",
    "mongoid-js": "^1.3.0",
    "normalize.css": "^8.0.1",
    "ol": "^6.2.1",
    "resize-sensor": "0.0.6",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rxjs": "^6.5.4",
    "uuid": "^7.0.1"
  }
}
